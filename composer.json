{
    "name": "yappabe/google-map-bundle",
    "type": "library",
    "description": "Provides a google map integration for your Symfony2 Project.",
    "keywords": [ "google map" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/symfony": "^4.0",
        "yappabe/google-map": "^1.4",
        "egeloen/json-builder": "dev-master",
        "php-http/guzzle6-adapter": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "twig/twig": "~1.12",
        "widop/http-adapter-bundle": "~1.1.0",
        "willdurand/geocoder": "~2.0",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-4": { "Ivory\\GoogleMapBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:eikona-media/ivory-json-builder.git"
        }
    ]
}
